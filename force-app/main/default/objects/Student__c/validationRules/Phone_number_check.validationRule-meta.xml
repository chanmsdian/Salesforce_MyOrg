<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_number_check</fullName>
    <active>true</active>
    <errorConditionFormula>NOT (REGEX(Contact_Number__c, &quot;\\D*?(\\d\\D*?){10}&quot;))</errorConditionFormula>
    <errorMessage>Contact number should be 10 Digits</errorMessage>
</ValidationRule>
