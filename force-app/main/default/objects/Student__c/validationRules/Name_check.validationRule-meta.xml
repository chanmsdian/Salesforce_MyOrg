<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Name_check</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(Student_First_Name__c  ,&quot;^[a-z  A-Z]*$&quot;))</errorConditionFormula>
    <errorDisplayField>Student_First_Name__c</errorDisplayField>
    <errorMessage>Please provide only alphabets</errorMessage>
</ValidationRule>
